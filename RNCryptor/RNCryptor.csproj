<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>netstandard2.0</TargetFramework>
		<Authors>$(AssemblyName) authors</Authors>
		<Company>$(Authors)</Company>
		<Copyright>$(Authors)</Copyright>
		<Description>C# implementation of Rob Napier's RNCryptor</Description>
		<RepositoryUrl>https://github.com/alexalok/RNCryptor-cs</RepositoryUrl>
		<RepositoryType>git</RepositoryType>
		<PackageTags>rncrypt;rncryptor;rncryptor-cs</PackageTags>
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
	</PropertyGroup>

</Project>